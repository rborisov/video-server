#!/usr/bin/gnuplot
set terminal png size 1280,720
set xdata time
#set timefmt "%b-%d-%H:%M:%S"
#set timefmt "%Y-%m-%d %H:%M:%S"
set timefmt "%H:%M:%S"
set output "/media/rmaidan/room_temp.png"
# time range must be in same format as data file
#set xrange ["Mar-25-00:00:00":"Mar-26-00:00:00"]
#set yrange [0:50]
set grid
set xlabel "Time"
set ylabel "Temp"
set title "rMaidan Temp"
set key left box
plot "/root/.temp_info" using 2:5 index 0 title "rMaidan Temp" with lines
